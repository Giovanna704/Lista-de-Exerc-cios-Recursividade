#include <stdio.h>
// 16. Impressão de números ímpares: Escreva uma função recursiva em C que imprima todos os números ímpares de `1` até `n` (inclusive).  
if (n < 0) {
return;
}
imprimir_impares(n - 1);
if (n % 2 != 0) {
printf("%d ", n);
}
}

int main() {
int n=20;

printf("Números impares de 0 até %d: ", n);
imprimir_impares(n);
printf("\n");

return 0;
}
